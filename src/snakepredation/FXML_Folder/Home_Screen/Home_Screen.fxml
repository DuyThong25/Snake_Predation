<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="snakepredation.FXML_Folder.Home_Screen.Home_ScreenController">
    <stylesheets>
        <URL value="@home_screen.css" />
    </stylesheets>
   <children>
      <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="background_Home" fitHeight="700.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
               <viewport>
                  <Rectangle2D />
               </viewport>
               <image>
                  <Image url="@../../../asset/image/background.jpg" />
               </image>
            </ImageView>
            <FlowPane alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0" StackPane.alignment="CENTER">
               <children>
                  <Button fx:id="startBtn_Home" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#MoveTo_PlayScreen" prefHeight="45.0" prefWidth="110.0" text="START" textAlignment="CENTER">
                     <FlowPane.margin>
                        <Insets bottom="20.0" />
                     </FlowPane.margin>
                  </Button>
                  <Button fx:id="ratingBtn_Home" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#MoveTo_RatingScreen" prefHeight="45.0" prefWidth="110.0" text="RATING" textAlignment="CENTER" />
               </children>
            </FlowPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
