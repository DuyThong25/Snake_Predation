<AnchorPane fx:id="anchorPane" prefHeight="800.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="snakepredation.FXML_Folder.Play_Screen.Play_ScreenController">
    <StackPane fx:id="stackPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <stylesheets>
            <URL value="@play_screen.css" />
        </stylesheets>
      <children>
         <Label fx:id="handleScores" prefHeight="59.0" prefWidth="130.0" text="SCORE: 0" textAlignment="CENTER" StackPane.alignment="TOP_LEFT">
